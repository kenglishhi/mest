<div class="active-scaffold active-scaffold-blast blast-view default-theme">
  <% form_for :blast_command, :url=> {:action => 'blast_clean'}  do |f | -%>
  <%= error_messages_for :blast_command  %>
<%= error_messages_for :term  %>
<div class="new-view blast-view view">
  <h4>Clean Raw Files with Blast </h4>
<div class="messages-container" id="blast-create--messages"/>
<table class='blast-main-form-table' border=1  >
<tr><td>
  <%= f.hidden_field  :biodatabase_name %>
  <%= f.hidden_field  :biodatabase_type_id %>
	<table class='blast-sub-form-table' >
	<tr>
		<th>Test File</th>
		<td><%= f.select  :test_fasta_file_id, @fasta_file_options,{} %></td>
	</tr>
  <tr>
		<th>E Value</th>
		<td><%= f.text_field :evalue%></td>
	</tr>
	<tr>
		<th>Identity</th>
		<td><%= f.text_field :identity  %></td>
	</tr>
	<tr>
		<th>Score</th>
		<td><%= f.text_field :score  %></td>
	</tr>

</table>
</td></tr>
<tr><td colspan=2>
  <p class="form-footer">
    <%= submit_tag 'Run Query', :class => 'submit' %>
  </p>
  <p class="form-footer">
		This will removed matching sequences creat
  </p>
</td></tr>
</table>

</div>
</div>
  <% end -%>
